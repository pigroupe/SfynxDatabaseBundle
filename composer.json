{
  "name": "sfynx-project/tool-database-bundle",
  "type": "symfony-bundle",
  "description": "This Bundle provides classes and commands to run DB vendor`s utilities to backup and restore databases.",
  "keywords": ["database", "backup", "restore", "dump"],
  "homepage": "http://pigroupe.github.io/cmf-sfynx/",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Etienne de Longeaux",
      "email": "etienne.delongeaux@gmail.com"
    },
    {
      "name": "Sfynx Community",
      "homepage": "https://github.com/pigroupe/SfynxDatabaseBundle/contributors"
    }
  ],

  "require": {
    "symfony/framework-bundle": "~2.8|~3.0|~4.0",
    "symfony/console": "*",
    "doctrine/orm": "^2.5",
    "sfynx-project/tool-utils-bundle": "~2.8.1"
  },

  "require-dev": {
    "symfony/phpunit-bridge": "~2.7|~3.0",
    "phpunit/phpunit": "*"
  },

  "suggest": {
  },

  "conflict": {
  },

  "autoload": {
    "psr-0": { "Sfynx\\DatabaseBundle": "" }
  },

  "target-dir": "Sfynx/DatabaseBundle",

  "extra": {
    "branch-alias": {
      "dev-master": "2.3-dev"
    }
  }
}
